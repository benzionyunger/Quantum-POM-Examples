<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Quantum POM Demo Suite" verbose="0" parallel="false">
    <listeners>
        <listener class-name="com.quantum.listerners.QuantumReportiumListener" />
    </listeners>

    <test name="IOS Test" enabled="false">
        <parameter name="driver.capabilities.model" value="iPhone.*"></parameter>
        <classes>
            <class name="com.quantum.RentalPropertyManagerTest" />
        </classes>
    </test>

    <test name="Android Test" enabled="true">
        <parameter name="driver.capabilities.deviceName" value="9889DB43574856364C"></parameter>
        <parameter name="env.resources" value="src/main/resources/android"></parameter>
        <classes>
            <class name="com.quantum.RentalPropertyManagerTest" />
        </classes>
    </test>
</suite>